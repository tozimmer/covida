		<y4>1650</y4>
		<screenx1>0</screenx1>
		<screeny1>0</screeny1>
		<screenx2>1152</screenx2>
		<screeny2>864</screeny2>
	</pencalibration>
	<storeevents>false</storeevents>
	<cacheDirectory>cache</cacheDirectory>
	<peninputdevice>
		<width>1680</width>
		<height>1050</height>
		<devicetype>WM_PEN</devicetype>
	</peninputdevice>
