<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<touchAndWriteConfiguration>
    <logConfig>log4j.xml</logConfig>
    <tuioconfiguration>
        <tuioPort>3333</tuioPort>
        <tuioTarget>localhost</tuioTarget>
        <delta>0.001</delta>
    </tuioconfiguration>
    <eventmanagerconfig>
        <inprocess>true</inprocess>
        <maximumframerate>25</maximumframerate>
        <eventport>6666</eventport>
        <controlport>6667</controlport>
        <moveDelta>0.001</moveDelta>
        <continousPenStreaming>false</continousPenStreaming>
        <intervalCheck>800</intervalCheck>
        <analyzerDelay>500</analyzerDelay>
        <channeltype>tcp</channeltype>
    </eventmanagerconfig>
    <pencalibration>
        <x1>0</x1>
        <y1>0</y1>
        <x2>1280</x2>
        <y2>0</y2>
        <x3>0</x3>
        <y3>800</y3>
        <x4>1280</x4>
        <y4>800</y4>
        <screenx1>0</screenx1>
        <screeny1>0</screeny1>
        <screenx2>1280</screenx2>
        <screeny2>800</screeny2>
    </pencalibration>
    <storeevents>false</storeevents>
    <cacheDirectory>cache</cacheDirectory>
    <peninputdevice>
        <width>1280</width>
        <height>800</height>
        <devicetype>WM_PEN</devicetype>
    </peninputdevice>
    <touchinputdevice>
        <width>1280</width>
        <height>800</height>
        <devicetype>WM_TOUCH</devicetype>
    </touchinputdevice>
</touchAndWriteConfiguration>
