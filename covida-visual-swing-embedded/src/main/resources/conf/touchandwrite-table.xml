<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<touchAndWriteConfiguration>
    <logConfig>log4j.xml</logConfig>
    <tuioconfiguration>
        <tuioPort>3333</tuioPort>
        <tuioTarget>localhost</tuioTarget>
        <delta>0.001</delta>
    </tuioconfiguration>
    <eventmanagerconfig>
        <inprocess>true</inprocess>
        <maximumframerate>50</maximumframerate>
        <eventport>6666</eventport>
        <controlport>6667</controlport>
        <moveDelta>0.001</moveDelta>
        <continousPenStreaming>false</continousPenStreaming>
        <intervalCheck>1000</intervalCheck>
        <analyzerDelay>500</analyzerDelay>
        <channeltype>tcp</channeltype>
    </eventmanagerconfig>
    <pencalibration>
        <x1>255</x1>
        <y1>397</y1>
        <x2>4111</x2>
        <y2>389</y2>
        <x3>208</x3>
        <y3>2794</y3>
        <x4>4141</x4>
        <y4>2764</y4>
        <screenx1>100</screenx1>
        <screeny1>90</screeny1>
        <screenx2>1180</screenx2>
        <screeny2>775</screeny2>
    </pencalibration>
    <storeevents>true</storeevents>
    <cacheDirectory>cache</cacheDirectory>
	
    <peninputdevice>
        <width>1280</width>
        <height>800</height>
        <devicetype>ANOTO</devicetype>
    </peninputdevice>
    <touchinputdevice>
        <width>1280</width>
        <height>800</height>
        <devicetype>TUIO</devicetype>
    </touchinputdevice>
</touchAndWriteConfiguration>
